var noiframedialog ;       
function opendialog_no_iframe(title,src,width,height,callbackfunc){
  if(!noiframedialog){
    var tag = $("<div class='ajaxdialog'></div>");
    jQuery.ajax({
      url: src,
      success: function(data) { 
      
       opendialog(title
       ,{content:tag.html(data).html()}
       ,width,height,{});
      
      #*
        noiframedialog  = tag.html(data).dialog({
			autoOpen: false,
			width:width,
			height:height,
			title: title,
			modal: true,
			autoResize:false,
			close: callbackfunc
		});
		noiframedialog.dialog('open'); *#
      }
    });
    return false;
  }
  $('.ajaxdialog .msg').empty();
  noiframedialog.dialog('open');
  return false;
}

function opendialog(title,src,width,height,callbackfunc){
   try
{  
  var footer =  $("#modal .modal-footer");
  if( callbackfunc && (callbackfunc.onSuccess || callbackfunc.onClose )){
 
     footer.css("display","block");
    
     footer.find("button").each(function(){
       $(this).unbind();
     });             
    
     if( callbackfunc.onSuccess){
       footer.find(".footer-submit").click(callbackfunc.onSuccess);
     }
  
     if( callbackfunc.onClose){
       footer.find(".footer-close").click(callbackfunc.onClose);
     }
  }else{
     footer.css("display","none");
  }


   if(src.content){
     $("#modal .modal-body").html("<div style='height:"+(height)+"'>"+src.content+"</div>");
   }else{
     $("#modal .modal-body").html("<div style='height:"+(height)+"'> <iframe id='wdialog' frameborder='0' height='"+(height-60)+"' width='100%' src='"+src+"' ></iframe></div>");
   } 
   $("#modal .modal-dialog").css("width",width);
   $("#modal #myModalLabel").html(title);
   $("#modal").modal({});
  #* 
     var dialog = $("<div style='height:"+(height)+"'> <iframe id='wdialog' frameborder='0' height='"+(height-60)+"' width='100%' src='"+src+"' ></iframe></div>")
		.dialog({
			autoOpen: false,
			width:width,
			height:height,
			title: title,
			modal: true,
			autoResize:false,
			//close: function(event, ui) { 
			//  if(!notRefeshWhenDialogClose){ 
			//     window.location.reload();
			//  }  
			//}
			close: callbackfunc
		});
	 
    dialog.dialog('open');
    return dialog;
    *#
}catch(err)
{
   alert(err);
}
   }
   
   function showMessage(data){
	    $("#msgblock").html("");
		if(data.errormsg.length>0){
		  var errorul = $("<ul class='errorMessage'></ul>");				  	
		  for(var i =0;i<data.errormsg.length;i++){
			errorul.append($("<li>"+data.errormsg[i]+"</li>"));
		  }
		  $("#msgblock").append(errorul);
	    }
				  	
		if(data.msg.length >0){
		  var msgul = $("<ul class='actionMessage'></ul>");
		  for(var i =0;i<data.msg.length;i++){
			 msgul.append($("<li>"+data.msg[i]+"</li>")); 
		  }
		  $("#msgblock").append(msgul);
		}
	}
   
   
   function appendMessage(data,container){
   
   
	   if(data.errormsg.length>0){
		  var errorul = $("<ul class='errorMessage'></ul>");				  	
		  for(var i =0;i<data.errormsg.length;i++){
			errorul.append($("<li>"+data.errormsg[i]+"</li>"));
		  }
		  container.append(errorul);
	    }
				  	
		if(data.msg.length >0){
		  var msgul = $("<ul class='actionMessage'></ul>");
		  for(var i =0;i<data.msg.length;i++){
			 msgul.append($("<li>"+data.msg[i]+"</li>")); 
		  }
		  container.append(msgul);
		}
	}
	
	function showMessageDialog(data){
	   var content = $("<div></div>");
	   appendMessage(data,content);
	   open_dialog("服务器执行结果",content,600);
	}
   
